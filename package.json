    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "clean-css-cli": "^4.3.0",
    "jasmine": "^2.99.0",
    "jasmine-core": "^2.99.1",
    "jasmine-reporters": "^2.3.2",
    "jsdom": "^8.5.0",
    "lodash": "^4.17.15",
    "prettier": "^2.0.4",
    "react": "~16.9.0",
    "react-addons-test-utils": "~15.3.2",
    "react-dom": "~16.9.0",
    "watchify": "^3.7.0",
    "webpack": "^4.42.1"
  },
  "scripts": {
    "test": "jasmine JASMINE_CONFIG_PATH=test/jasmine.json",
    "build": "browserify ./src/s-forms.js -o ./dist/s-forms.js",
    "watchify": "watchify ./src/s-forms.js -o ./dist/s-forms.js",
    "lib": "babel ./src -d ./lib",
    "build-npm-prod": "npm run build && npm run lib && npm pack",
    "build-css": "cleancss --skip-rebase -d -o ./css/s-forms.min.css ./css/s-forms.css",
    "rendering-test-start": "watchify -o test/rendering/bundle.js -v -d test/rendering/TestApp.js",
    "prebuild-npm-prod": "npm install"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
